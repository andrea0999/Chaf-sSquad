         <bottom>
            <Button fx:id="vizualizarePaginaReteta" mnemonicParsing="false" onAction="#handleVizualizarePaginaReteta" text="Vizualizare" BorderPane.alignment="CENTER" />
         </bottom>
            <Text fx:id="message" layoutX="406.0" layoutY="340.0" wrappingWidth="176.0">
